##############################################
# .tfignore
##############################################

# Ignore Packages folder
packages
# Do not ignore packages needed for .NET targetting
!packages/*.targets

# Ignore compilation output
bin
obj

# Ignore Visual Studio local files
*.user
# Ignore MS Test results
testresults
# Ignore .jfm SSDT Temp file
*.jfm
# Ignore primary source control (TFS/VSS)
*.vspscc


