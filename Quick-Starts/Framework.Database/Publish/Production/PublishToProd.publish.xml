<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="12.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IncludeCompositeObjects>True</IncludeCompositeObjects>
    <TargetDatabaseName>FrameworkData</TargetDatabaseName>
    <DeployScriptFileName>GoodToCode.Framework.Test.Database.sql</DeployScriptFileName>
    <TargetConnectionString>Data Source=DatabaseServer.prod.GoodToCode.com;Integrated Security=True;Pooling=False</TargetConnectionString>
    <ProfileVersionNumber>1</ProfileVersionNumber>
    <ScriptDatabaseOptions>True</ScriptDatabaseOptions>
    <BlockOnPossibleDataLoss>False</BlockOnPossibleDataLoss>
  </PropertyGroup>
  <ItemGroup>
    <SqlCmdVariable Include="TargetEnvironment">
      <Value>Production</Value>
    </SqlCmdVariable>
  </ItemGroup>
</Project>