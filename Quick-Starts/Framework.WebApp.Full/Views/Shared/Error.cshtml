@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

<h1 class="text-danger">Error.</h1>
<h2 class="text-danger">An error occurred while processing your request.</h2>
<div style="display:none;">@(Model != null && Model.Exception != null ? Model.Exception.Message : "")</div>
<div style="display:none;">@(Model != null && Model.Exception != null  && Model.Exception.InnerException != null ? Model.Exception.InnerException.Message : "")</div>
<div style="display:none;">@(Model != null && Model.Exception != null ? Model.Exception.StackTrace : "")</div>

