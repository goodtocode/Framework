<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>Genesys Source Framework Test Project</Name>
    <Description>Genesys Source Framework Test project containing unit tests for items in the templated projects. </Description>
    <ProjectType>CSharp</ProjectType>
    <ProjectSubType>
    </ProjectSubType>
    <CreateInPlace>true</CreateInPlace>
    <SortOrder>1000</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>Framework.Test.Core</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <Icon>__TemplateIcon.png</Icon>
    <PreviewImage>__PreviewImage.PNG</PreviewImage>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="Framework.Test.Core.csproj" File="Framework.Test.Core.csproj" ReplaceParameters="true">
      <Folder Name="Activity" TargetFolderName="Activity" />
      <Folder Name="App_Data" TargetFolderName="App_Data">
        <ProjectItem ReplaceParameters="true" TargetFileName="AppSettings.Debug.json">AppSettings.Debug.json</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="AppSettings.json">AppSettings.json</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="AppSettings.Release.json">AppSettings.Release.json</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="ConnectionStrings.Debug.json">ConnectionStrings.Debug.json</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="ConnectionStrings.json">ConnectionStrings.json</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="ConnectionStrings.Release.json">ConnectionStrings.Release.json</ProjectItem>
        <ProjectItem ReplaceParameters="false" TargetFileName="FrameworkData_Primary.mdf">FrameworkData_Primary.mdf</ProjectItem>
        <ProjectItem ReplaceParameters="false" TargetFileName="PostBuild.Debug.bat">PostBuild.Debug.bat</ProjectItem>
        <ProjectItem ReplaceParameters="false" TargetFileName="PostBuild.Release.bat">PostBuild.Release.bat</ProjectItem>
        <ProjectItem ReplaceParameters="false" TargetFileName="PreBuild.Debug.bat">PreBuild.Debug.bat</ProjectItem>
        <ProjectItem ReplaceParameters="false" TargetFileName="PreBuild.Release.bat">PreBuild.Release.bat</ProjectItem>
      </Folder>
      <Folder Name="Application" TargetFolderName="Application" />
      <Folder Name="Data" TargetFolderName="Data" />
      <Folder Name="Endpoints" TargetFolderName="Endpoints" />
      <Folder Name="Entity" TargetFolderName="Entity" />
      <Folder Name="Model" TargetFolderName="Model" />
      <Folder Name="Serialization" TargetFolderName="Serialization" />
      <Folder Name="Validation" TargetFolderName="Validation" />
      <Folder Name="Worker" TargetFolderName="Worker" />
      <ProjectItem ReplaceParameters="true" TargetFileName="App.json">App.json</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="appsettings.json">appsettings.json</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="appsettings.Development.json">appsettings.Development.json</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="appsettings.Local.json">appsettings.Local.json</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="appsettings.Production.json">appsettings.Production.json</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="appsettings.Staging.json">appsettings.Staging.json</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="ConfigurationManagerLocal.cs">ConfigurationManagerLocal.cs</ProjectItem>
    </Project>
  </TemplateContent>
</VSTemplate>